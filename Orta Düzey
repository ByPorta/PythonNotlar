#%%
def uzunluk(k):
    return len(k)
meyve=["portakal","mango","kivi","ananas","muz"]
meyve.sort(key=uzunluk,reverse=True)
print(meyve)
#%%
def yirmi(n):
    return abs(n-20)
sayi=[2457,11,9,568,3,1,87]
sayi.sort(key=yirmi,reverse=True)
print(sayi)

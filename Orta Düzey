#%% 
toplam=0
sayı=int(input("bir sayı giriniz:"))
for i in range(sayı+1):
    toplam=toplam+i
    print("sayıların toplamı:",toplam)
#printten önce boşluk olmaz ise direkt toplam sonucu yazar.
#%% 
toplam=0
ogrencisay=int(input("öğrenci sayısını giriniz:"))
for i in range(ogrencisay):
    ogrencinot=int(input(f"{i+1}.öğrencinin notunu giriniz:"))
    toplam+=ogrencinot
if ogrencisay>0:
    print(f"öğrencilerin ortalaması:{toplam/ogrencisay}")
else:
    print("ortalama hesaplanamaz...")
#%% 
print ("20 ile 60 arasında son rakamı 3,5,7 olan sayılar:")
for i in range (20,60):
    if i%10==3 or i%10==5 or i%10==7:
        print(i,end=" ")
#%%
enbuyuk=0
sayi=int(input("sayıyı giriniz: "))
while sayi>0:
    if sayi > enbuyuk:
        enbuyuk=sayi
    sayi = int(input("sayıyı giriniz: "))
print("en büyük sayı:",enbuyuk)

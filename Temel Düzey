#%%

dolar_kuru = 26.8
euro_kuru = 29.5

tl_miktarı = 2400

dolar_alınabilir = tl_miktarı / dolar_kuru
euro_alınabilir = tl_miktarı / euro_kuru

print(f"{tl_miktarı} TL ile {dolar_alınabilir:.2f} dolar alabilirsiniz.")
print(f"{tl_miktarı} TL ile {euro_alınabilir:.2f} euro alabilirsiniz.")

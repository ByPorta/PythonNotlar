#%%
tutar=float(input("alışveriş tutarını giriniz:"))
if tutar<500:
    odenecek=tutar
elif tutar<1001:
    odenecek=tutar*0.95
elif tutar<2000:
    odenecek=tutar*0.90
else:
    odenecek=tutar*0.80
print("alışveriş tutarı:",tutar)
print("indirim miktarı:{}".format(tutar-odenecek))
print(f"odenecek tutar:{odenecek}")
#%%
ilk_sayı=int(input("ilk sayıyı giriniz:"))
ikinci_sayı=int(input("ikinci sayıyı giriniz:"))
üçüncü_sayı=int(input("üçüncü sayıyı giriniz:"))

if ilk_sayı>ikinci_sayı and ilk_sayı>üçüncü_sayı:
    print("en büyük sayı ilk girdiğiniz sayıdır.")
elif ikinci_sayı>ilk_sayı and ikinci_sayı>üçüncü_sayı:
    print("en büyük sayı ikinci girdiğiniz sayıdır.")
elif üçüncü_sayı>ilk_sayı and üçüncü_sayı>ikinci_sayı:
    print("en yüksek sayı üçüncü sayıdır.")
#%%
from math import sqrt
a=int(input("a değeri giriniz:"))
b=int(input("b değerini giriniz:"))
c=int(input("c değerini giriniz:"))
delta=pow(b,2)-4*a*c
print("delta değeri:{}".format(delta))
if delta <0:
    print("sanal kök") 
elif delta==0:
    x1=-b/(2*a)
    print("x1={}".format(x1))
else:
    x1=(-b-sqrt(delta))/(2*a)
    x2=(-b+sqrt(delta))/(2*a)
    print("iki kök hesaplandı.")
    print("x1={}".format(x1))
    print("x2={}".format(x2))
#%%
vize=int(input("vize notunu giriniz:"))
final=int(input("final notunu giriniz:"))
dönemsonu=vize*0.40+final*0.60
print(f"dönem sonu notunuz:{dönemsonu}")
if dönemsonu>=60:
    print("geçtiniz")
else:
    print("kaldınız.MAL")
if dönemsonu>=90:
    harfnotu="A"
elif dönemsonu>=80 and dönemsonu<90:
    harfnotu="B"
elif dönemsonu>=70 and dönemsonu<80:
    harfnotu="C"
elif dönemsonu>=60 and dönemsonu<70:
    harfnotu="D"
else:
    harfnotu="FF"
print("harf notunuz:",harfnotu)

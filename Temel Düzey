#%% 
toplam=0
i=1 
ogrencinot=int(input(f"{i}.öğrencinin notunu giriniz:"))
while ogrencinot>0:
    toplam=toplam+ogrencinot
    i+=1 
    ogrencinot=int(input(f"{i}.ogrencinin notunu giriniz:"))
if i>1:
    print("ogrencilerin not ortalaması:",toplam/(i-1))
else:
    print("ortalaması hesaplanacak örğenci yok")
